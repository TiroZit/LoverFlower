extends _html/_base
include _html/_mixins
block vars
  - var lang = 'ru'
  - var type = 'website'
  - var title = 'Ошибка 404 | LoverFlower'
  - var description = 'Цветочный магазин'
  - var siteName = 'LoverFlower'
  - var keywords = 'lover flower loverflower lf цветы магазин'
